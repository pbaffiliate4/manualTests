var docvar =""; 
var ts = apsdb.beginTransaction();
var documents = require("document");
var storeName = "DefaultStore";
var myDoc = documents.getInstance(storeName);
var docID = "batata";
docvar = myDoc.get(docID, {"includeFieldType":"true", "lock":"true"});
while(true){
    
}
ts.commit(); 
return docvar;